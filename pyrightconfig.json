{
    "include": [
        "src",
        "scripts"
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        "**/.git",
        "**/logs",
        "**/backup"
    ],
    "venvPath": ".",
    "venv": "venv",
    "pythonVersion": "3.13",
    "pythonPlatform": "Darwin",
    "executionEnvironments": [
        {
            "root": "src/django_api",
            "pythonVersion": "3.13",
            "pythonPlatform": "Darwin",
            "extraPaths": [
                "../../venv/lib/python3.13/site-packages"
            ]
        },
        {
            "root": "src",
            "pythonVersion": "3.13",
            "pythonPlatform": "Darwin"
        }
    ],
    "reportMissingImports": "warning",
    "reportMissingTypeStubs": false,
    "typeCheckingMode": "basic",
    "reportAttributeAccessIssue": "warning"
}
